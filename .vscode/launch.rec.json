    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch",
            "cwd": "${workspaceFolder}",
            "terminal": "console",
            // run
            "program": "${workspaceFolder}/build/bin/buddy-opt",
            "args": [
                "-lower-rvv",
                "examples/RVVDialect/rvv-setvl.mlir",
            ],
            "initCommands": [
                "settings set target.disable-aslr false",
                // "command source ${workspaceRoot}/.lldbinit",
                // "settings set target.process.thread.step-avoid-regexp (.)std(.)*",
            ],
            "env": {
                // "GLOG_v": "10",
            },
        },
    ]
}
